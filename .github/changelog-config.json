{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement", "feat"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix", "bugfix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ§ª Testing",
      "labels": ["test", "testing"]
    },
    {
      "title": "## âš¡ Performance",
      "labels": ["performance", "perf"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["maintenance", "chore", "refactor"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## ğŸ“¦ Dependencies",
      "labels": ["dependencies", "deps"]
    }
  ],
  "ignore_labels": [
    "ignore-for-release",
    "duplicate",
    "question",
    "invalid",
    "wontfix"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)(\\(.+\\))?!?:",
      "target": "feature"
    },
    {
      "pattern": "^(fix|bugfix)(\\(.+\\))?!?:",
      "target": "bug"
    },
    {
      "pattern": "^(docs|doc)(\\(.+\\))?!?:",
      "target": "documentation"
    },
    {
      "pattern": "^(test|tests)(\\(.+\\))?!?:",
      "target": "test"
    },
    {
      "pattern": "^(perf|performance)(\\(.+\\))?!?:",
      "target": "performance"
    },
    {
      "pattern": "^(chore|refactor|style)(\\(.+\\))?!?:",
      "target": "maintenance"
    },
    {
      "pattern": "^(security|sec)(\\(.+\\))?!?:",
      "target": "security"
    },
    {
      "pattern": "^(deps|dependencies)(\\(.+\\))?!?:",
      "target": "dependencies"
    }
  ],
  "transformers": [
    {
      "pattern": "\\[(.+)\\]",
      "target": "**$1**"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [
    "main",
    "master",
    "develop"
  ]
} 